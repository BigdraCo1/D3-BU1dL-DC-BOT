{
  "name": "discord-bot",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "lint": "tsc --noEmit",
    "deploy": "bun run src/deploy-commands.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "prisma": "^6.19.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3"
  }
}
